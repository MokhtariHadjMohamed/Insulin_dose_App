            android:text="قياس السكر"
            android:textAlignment="center"
            android:textColor="@color/text02"
            android:textSize="20dp" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView0"
        android:layout_width="136dp"
        android:layout_height="131dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="24dp"
        android:background="#00FFFFFF"
        app:cardCornerRadius="20dp"
        app:cardElevation="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView1"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="79dp"
            android:layout_height="93dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="5dp"
            app:srcCompat="@drawable/pharmaceuticalh"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="146dp"
            tools:layout_editor_absoluteY="28dp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="134dp"
            android:layout_height="31dp"
            android:layout_marginTop="95dp"
            android:fontFamily="@font/tajawal_bold"
            android:letterSpacing="0.02"
            android:text="الأدوية"
            android:textAlignment="center"
            android:textColor="@color/text02"
            android:textSize="20dp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView1"
        android:layout_width="136dp"
        android:layout_height="131dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="16dp"
        android:background="#00FFFFFF"
        app:cardCornerRadius="20dp"
        app:cardElevation="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView7"
        tools:ignore="MissingConstraints">


        <TextView
            android:id="@+id/textView14"
            android:layout_width="134dp"
            android:layout_height="31dp"

            android:layout_marginTop="99dp"
            android:fontFamily="@font/tajawal_bold"
            android:gravity="fill_vertical"
            android:letterSpacing="0.02"
            android:text="الرياضة"
            android:textAlignment="center"
            android:textColor="@color/text02"
            android:textSize="20dp" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            app:srcCompat="@drawable/sporth"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="146dp"
            tools:layout_editor_absoluteY="28dp" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/menu"
        android:layout_width="57dp"
        android:layout_height="47dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="50dp"
        app:cardCornerRadius="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView1"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="44dp"
            android:layout_height="28dp"
            android:layout_gravity="center"
            android:elevation="4dp"
            app:srcCompat="@drawable/menu"
            tools:ignore="MissingConstraints" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView6"
        android:layout_width="300dp"
        android:layout_height="95dp"
        android:layout_marginTop="8dp"
        android:background="#00FFFFFF"
        app:cardCornerRadius="20dp"
        app:cardElevation="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView5"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="55dp"

            android:layout_marginTop="5dp"
            android:fontFamily="@font/tajawal_bold"
            android:text="مجتمع جرعة انسولين "
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="190dp"
            android:layout_height="wrap_content"

            android:layout_marginLeft="10dp"
            android:layout_marginTop="28dp"
            android:fontFamily="@font/tajawal_medium"
            android:gravity="right|top"
            android:letterSpacing="0.02"
            android:text="مجتمع انسولين يساعد مرضى السكري على تبادل الآراء والمعلومات "
            android:textColor="@color/text02"
            android:textSize="16dp" />

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="90dp"
            android:layout_height="84dp"
            android:layout_marginLeft="210dp"
            android:layout_marginTop="10dp"
