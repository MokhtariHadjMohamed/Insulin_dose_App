<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AccountActivity">

    <EditText
        android:id="@+id/nameRegisterActivity2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/edit_text_shap_rec"
        android:drawableEnd="@drawable/user_solid"
        android:drawablePadding="10dp"
        android:drawableTint="@color/color6"
        android:elevation="10dp"
        android:ems="10"
        android:gravity="center|right"
        android:hint="الإسم الكامل"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView61" />

    <EditText
        android:id="@+id/emailRegisterActivity2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/edit_text_shap_rec"
        android:drawableEnd="@drawable/envelope_solid"
        android:drawablePadding="10dp"
        android:drawableTint="@color/color6"
        android:elevation="10dp"
        android:ems="10"
        android:gravity="center|right"
        android:hint="البريد الإلكتروني"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nameRegisterActivity2" />

    <EditText
        android:id="@+id/phoneRegisterActivity2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/edit_text_shap_rec"
        android:drawableEnd="@drawable/baseline_call_24"
        android:drawablePadding="10dp"
        android:drawableTint="@color/color6"
        android:elevation="10dp"
        android:ems="10"
        android:gravity="center|right|end"
        android:hint="رقم هاتف"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailRegisterActivity2" />

    <EditText
        android:id="@+id/passwordRegisterActivity2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/edit_text_shap_rec"
        android:drawableEnd="@drawable/lock_solid"
        android:drawablePadding="10dp"
        android:drawableTint="@color/color6"
        android:elevation="10dp"
        android:ems="10"
        android:gravity="center|right"
        android:hint="كلمة المرور"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phoneRegisterActivity2" />

    <EditText
        android:id="@+id/password2RegisterActivity2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/edit_text_shap_rec"
        android:drawableEnd="@drawable/lock_solid"
        android:drawablePadding="10dp"
        android:drawableTint="@color/color6"
        android:elevation="10dp"
        android:ems="10"
        android:gravity="center|right"
        android:hint="تأكيد كلمة المرور"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passwordRegisterActivity2" />

    <TextView
        android:id="@+id/textView61"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/tajawal_medium"
        android:text="تعديل الملف الشخصي"
        android:textColor="@color/text10"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView58" />

    <TextView
        android:id="@+id/textView59"
        android:layout_width="149dp"
        android:layout_height="54dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="36dp"
        android:background="@drawable/rectangle_7"
        android:drawableEnd="@drawable/cancel"
        android:drawablePadding="15dp"
        android:elevation="10dp"
        android:fontFamily="@font/tajawal_medium"
        android:gravity="center_vertical"
        android:paddingRight="20dp"
        android:text="إلغاء"
        android:textAlignment="gravity"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView40" />

    <TextView
        android:id="@+id/textView60"
        android:layout_width="149dp"
        android:layout_height="54dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/rectangle_7"
        android:drawableEnd="@drawable/add"
        android:drawablePadding="15dp"
        android:elevation="10dp"
        android:fontFamily="@font/tajawal_medium"
        android:gravity="center_vertical"
        android:paddingRight="20dp"
        android:text="إضافة"
        android:textAlignment="gravity"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView59"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="@+id/textView59" />

    <TextView
        android:id="@+id/textView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:fontFamily="@font/tajawal_bold"
        android:shadowColor="@color/colorshadow"
        android:shadowDx="0"
        android:shadowDy="4"
        android:shadowRadius="4"
        android:text="تعديل الحساب"
        android:textColor="@color/text02"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView39"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView58"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView33"
        app:srcCompat="@drawable/baseline_arrow_back_ios_new_24" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="149dp"
        android:layout_height="54dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/rectangle_77"
        android:drawableEnd="@drawable/kitchen"
        android:drawablePadding="15dp"
        android:elevation="20dp"
        android:fontFamily="@font/tajawal_medium"
        android:gravity="center_vertical"
        android:paddingRight="20dp"
        android:text="الوزن"
        android:textAlignment="gravity"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ellipse_"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="149dp"
        android:layout_height="54dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/rectangle_77"
        android:drawableEnd="@drawable/height"
        android:drawablePadding="15dp"
        android:elevation="20dp"
        android:fontFamily="@font/tajawal_medium"
        android:gravity="center_vertical"
        android:paddingRight="20dp"
        android:text="الطول"
        android:textAlignment="gravity"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView40"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView40"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/ellipse_"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="80dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/ellipse_13"
        android:elevation="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/password2RegisterActivity2"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/ellipse_2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="72dp"
        android:background="@drawable/ellipse_15"
        android:elevation="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/ellipse_"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ellipse_"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/bolivian_gi"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/gender_male"
        app:layout_constraintBottom_toBottomOf="@+id/ellipse_2"
        app:layout_constraintEnd_toEndOf="@+id/ellipse_2"
        app:layout_constraintStart_toStartOf="@+id/ellipse_2"
        app:layout_constraintTop_toTopOf="@+id/ellipse_2"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/bolivian_gi2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/gender_female"
        app:layout_constraintBottom_toBottomOf="@+id/ellipse_"
        app:layout_constraintEnd_toEndOf="@+id/ellipse_"
        app:layout_constraintStart_toStartOf="@+id/ellipse_"
        app:layout_constraintTop_toTopOf="@+id/ellipse_"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>